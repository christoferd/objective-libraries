{
    "name": "christoferd/objective-libraries",
    "description": "This is my package objective-libraries",
    "keywords": [
        "christoferd",
        "objective-libraries"
    ],
    "homepage": "https://github.com/christoferd/objective-libraries",
    "license": "MIT",
    "authors": [
        {
            "name": "Christofer De David",
            "email": "chrisd@mindflow.com.au",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^3.2",
        "friendsofphp/php-cs-fixer": "^3.21.1",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Christoferd\\ObjectiveLibraries\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Christoferd\\ObjectiveLibraries\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
